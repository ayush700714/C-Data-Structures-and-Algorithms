#include<iostream>
using namespace std;
#define P INT_MAX
long long int arr[5][5]={{0,3,8,P,-4},{P,0,P,1,7},{P,4,0,P,P},{2,P,-5,0,P},{P,P,P,6,0}};
int main()
{
   long long int n;
    cout<<"enter number of vertices\n";
    cin>>n;
   long long int i,j,k;
  long long  int a[n][n],b[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            a[i][j]=arr[i][j];
            cout<<a[i][j]<<" ";
        }
        cout<<"\n";
    }
    cout<<"\n";
    for(k=0;k<n;k++)
    {
        for(i=0;i<n;i++)
        {
            for(j=0;j<n;j++)
            {
                b[i][j]=min(a[i][j],a[i][k]+a[k][j]);
                cout<<b[i][j]<<" ";
            }
            cout<<"\n";
        }
        cout<<"\n";
        for(i=0;i<n;i++)
        {
            for(j=0;j<n;j++)
            {
                a[i][j]=b[i][j];
            }
        }
    }
}
